{
  "name": "postduck-agent",
  "version": "0.0.1",
  "description": "Local agent for Postduck to enable localhost requests from the web app",
  "main": "dist/index.js",
  "bin": {
    "postduck-agent": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "package": "pkg . --targets node18-macos-arm64,node18-macos-x64,node18-linux-x64,node18-win-x64 --output-path ./dist/bin"
  },
  "keywords": [
    "postduck",
    "api",
    "proxy",
    "localhost"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24",
    "pkg": "^5.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "pkg": {
    "assets": [],
    "outputPath": "dist/bin"
  }
}
